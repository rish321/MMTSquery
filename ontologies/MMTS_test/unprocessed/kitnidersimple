PREFIX rdfs: <rdfsprefix>
PREFIX rdf: <rdfprefix>
PREFIX owl: <owlprefix>
PREFIX xsd: <xsdprefix>
PREFIX : <baseprefix>

SELECT ((xsd:time((?time1))-xsd:time(?time2)) AS ?durationTime)
#SELECT ?time1 ?time2 ?t1 ?t2 ?r1 ?r2
WHERE {
	    {SELECT ?time2 ?t2 ?r2
	    WHERE{
		    ?s2 :departsOn ?time2.
#		    FILTER ( ?time2 > "arrtimeinit:00"^^xsd:time ).
#		    FILTER ( ?time2 < "arrtimefinal:00"^^xsd:time ).
		    ?s2 :ofTrain ?t2.
		    ?s2 :atStation ?st2.
		    ?s2 :forRoute ?r2.
		    FILTER ( regex ( str(?st2), "depstation") ).
	    }}
	    .
	    {SELECT (set(?time) AS ?time1)
	    WHERE {
		    ?s1 :departsOn ?time.
		    FILTER ( ?time > "begtimedep:00"^^xsd:time ).
		    FILTER ( ?time < "endtimedep:00"^^xsd:time ).
		    ?s1 :ofTrain ?t1.
		    ?s1 :atStation ?st1.
		    FILTER ( regex ( str(?st1), "arrstation") ).
		    ?s1 :source ?src.
		    FILTER ( regex ( str(?src), "depstation") ).
		}
	    GROUP BY ?st1 }
	    .
	    ?s1 :ofTrain ?t1.
	    ?s1 :departsOn ?time1.
	    ?s1 :atStation ?st1.
	    ?s1 :source ?src.
	    FILTER ( regex ( str(?st1), "arrstation") ).
	    FILTER ( regex ( str(?src), "depstation") ).
	    FILTER ( ?t1 = ?t2).
	    FILTER ( ?time1 > ?time2 ).
	    ?s1 :forRoute ?r1
}
